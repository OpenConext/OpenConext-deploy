app.version={{ aa_version }}

server.contextPath=
logging.config=file://{{ aa_dir }}/logback.xml
server.port={{ springapp_tcpport }}

spring.profiles.active={{ aa.spring_profiles_active }}

scim.server.environment=test.surfconext
attribute.authorities.config.path=file://{{ aa_dir }}/attributeAuthorities.yml

# the authorization server url for obtaining a client_credential access_token
authorization.accessToken.url=https://authz.{{ base_domain }}/oauth/token

# Details needed so that we may check tokens presented to us by clients. This application uses them to authenticate via
# Basic authentication with the oAuth server.
authz.checkToken.endpoint.url=https://authz.{{ base_domain }}/oauth/check_token
authz.checkToken.clientId={{ aa.authz_resource_server_id }}
authz.checkToken.secret={{ aa.authz_secret }}

oidc.checkToken.endpoint.url=https://oidc.{{ base_domain }}/introspect
oidc.checkToken.clientId={{ aa.oidc_client_id }}
oidc.checkToken.secret={{ aa.oidc_secret }}

checkToken.cache=true
checkToken.cache.duration.milliSeconds=300000

# 8 hours
session-timeout=28800

spring.datasource.url=jdbc:mysql://{{ aa.db_host }}/{{ aa.db_name }}
spring.datasource.username={{ aa.db_user }}
spring.datasource.password={{ aa.db_password }}
spring.datasource.driverClassName=com.mysql.jdbc.Driver
spring.datasource.validation-query=SELECT 1
spring.datasource.testWhileIdle=true

attribute.aggregation.user.name={{ aa.eb_username }}
attribute.aggregation.user.password={{ aa.eb_password }}

metadata.username=metadata.client
metadata.password={{ metadata_exporter_password }}
metadata.refresh.minutes=10
metadata.spRemotePath=https://multidata.{{ base_domain }}/service-providers.json?includeTest=true

# 5 minutes
aggregate.cache.duration.milliseconds=300000

aa.private.
aa.public.
aa.entityId=https://engine.{{ base_domain }}/authentication/idp/metadata

endpoints.enabled=false
endpoints.health.enabled=true
endpoints.info.enabled=true

logging.level.aa=DEBUG

#enable for debugging information

#logging.level.org.springframework.security=DEBUG
#logging.level.org.hibernate.SQL=DEBUG
#logging.level.org.hibernate.type=TRACE
